	<table class="table table-hover">
		<col width="90">
		<thead>
			<th>Action</th>
			<th>ID</th>
			<th>First Name</th>
			<th>Last Name</th>
			<th>Email</th>
			<th class="note-column">Note</th>
			<!-- <th>Delete</th> -->
		</thead>
		<tbody>
			<% @contacts_to_search.each do |contact| %>
			<tr>
				<td><a href="/contacts/view/<%= contact.id %>">View</a></td>
				<td><%= contact.id %></td>
				<td><%= contact.first_name %></td>
				<td><%= contact.last_name %></td>
				<td><%= contact.email %></td>
				<td class="note-column"><%= contact.note %></td>
				<td><a href="/contacts/delete/<%= contact.id %>">Delete</a></td>
			</tr>
			<% end %>
		</tbody>
	</table>

<a href="/">Return to Main Menu</a>